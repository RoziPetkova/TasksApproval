<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core">
     <Button 
            icon="sap-icon://home" 
            tooltip="{i18n>Homepage}" 
            press=".onHomepagePress"
            visible="{= ${view>/showHomepageButton} !== false}" />
    <Button icon="sap-icon://settings" tooltip="{i18n>Settings}" press=".onSettingsPress" />
    <Button icon="sap-icon://log" tooltip="{i18n>LogOut}" press=".onLogoutPress" />
</core:FragmentDefinition>